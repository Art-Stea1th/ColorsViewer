<Page x:Class="ColorsViewer.Views.Pages.SystemColorsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Title="SystemColorsPage">
    
    <ScrollViewer>
        <ItemsControl
            IsTabStop="False"
            ItemTemplate="{StaticResource NamedColorDataTemplate}"
            ItemsSource="{Binding SystemColors}"/>
    </ScrollViewer>
</Page>